#!/bin/bash

PROG="./toy_mc.exe"
ODIR="."

######################## DATA ########################
SEEDS="121 122 123 124"
# "125 126 127 128"

printf "${SEEDS}" | \
    xargs --delimiter=" " --max-procs=4 -I{} \
      sh -c "${PROG} -n 500 -s {} \
            -h ${ODIR}/toymc_{}.root \
            > ${ODIR}/toymc_{}.out 2>&1"

